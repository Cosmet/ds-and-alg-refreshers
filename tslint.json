{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-airbnb"
  ],
  "jsRules": {},
  "rules": {
    "import-name": false,
    "max-line-length": [
      true,
      120
    ],
    "no-empty-interface": false,
    "ordered-imports": true,
    "object-literal-sort-keys": false,
    "quotemark": [
      true,
      "single"
    ],
    "semicolon": true,
    "no-console": false,
    "no-this-assignment": [
      true,
      {
        "allowed-names": [
          "^self$"
        ],
        "allow-destructuring": true
      }
    ],
    "align": [false, "parameters"],
    "space-before-function-paren": true,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "pascal-case"
    ],
    "ter-arrow-parens": [
      true,
      "always"
    ],
    "prefer-array-literal": false
  },
  "rulesDirectory": []
}
